---
import Layout from "../layouts/Layout.astro";
import { getPersonajes,getUniversos } from "../lib/personajes";
import FilterComponent from '../components/FiltroComponent'

const data = await getPersonajes();
const dataUni = await getUniversos();



---
<Layout title="Dragon Ball Data">
    <FilterComponent client:load data={data} dataUni={dataUni} />
</Layout>


